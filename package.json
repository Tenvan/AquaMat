{
  "name": "anqua-mat",
  "version": "1.0.0",
  "scripts": {
    "esp8266_watch": "watchexec -rcN -e yaml -w ./src 'yarn esp8266_generate'",
    "esp8266_config": "esphome config src/aquamat-es8226.yaml > .build/build-esp8266.yaml",
    "esp8266_compile": "esphome compile src/aquamat-es8226.yaml",
    "esp8266_run": "esphome run src/aquamat-es8226.yaml",
    "esp8266_generate": "esphome compile --only-generate src/aquamat-es8226.yaml",
    "esp32_config": "esphome config src/aquamat-es32.yaml > .build/build-esp32.yaml"
  },
  "private": true,
  "dependencies": {
  },
  "devDependencies": {
    "esphome-ts": "^3.3.0",
    "rimraf": "^3.0.2",
    "watch-files": "^1.1.0"
  }
}
